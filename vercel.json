{
    "version": 2,
    "builds": [
        {
            "src": "src/api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/api/index.ts"
        }
    ],
    "functions": {
        "src/api/index.ts": {
            "maxDuration": 60
        }
    },
    "crons": [
        {
            "path": "/api/agent/trigger",
            "schedule": "0 */2 * * *"
        }
    ]
}